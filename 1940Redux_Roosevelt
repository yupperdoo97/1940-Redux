campaignTrail_temp.running_mate_image_url = "https://imgur.com/ZJ3suvD.jpeg";
campaignTrail_temp.running_mate_last_name = "Hull";
}
if(ans == 2535){
campaignTrail_temp.running_mate_image_url = "https://imgur.com/mTxwUdV.jpeg";
campaignTrail_temp.running_mate_last_name = "Byrnes";
}
if(ans == 2536){
campaignTrail_temp.running_mate_image_url = "https://imgur.com/d4uRFiH.jpeg";
campaignTrail_temp.running_mate_last_name = "Bankhead";
}
else {
return false
}
  }

