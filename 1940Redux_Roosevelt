vpTable = {
    "Franklin D. Roosevelt" : vpName,
    "Thomas Dewey" : "John Bricker",
    "Unpledged Electors" : "N/A",
    "Others" : "various"
}

vpTableHistorical = {
   "Franklin D. Roosevelt" : "Harry Truman",
    "Thomas Dewey" : "John Bricker",
    "Unpledged Electors" : "N/A",
    "Others" : "various"
